{
  "testEnvironment": "node",
  "transform": {},
  "testMatch": [
    "**/tests/**/*.test.js"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/server.js",
    "!src/config/**/*.js"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ]
}